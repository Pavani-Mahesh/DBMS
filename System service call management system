CREATE TABLE Employees (
EmployeeID INT PRIMARY KEY,
Name VARCHAR(255) NOT NULL,
Department VARCHAR(100),
Position VARCHAR(100),
Phone VARCHAR(20),
Email VARCHAR(255)
);

