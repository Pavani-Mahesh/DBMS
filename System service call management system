CREATE TABLE ServiceCallHistory (
   HistoryID INT PRIMARY KEY,
   ServiceCallID INT FOREIGN KEY REFERENCES ServiceCalls(ServiceCallID),
   EmployeeID INT FOREIGN KEY REFERENCES Employees(EmployeeID),
   ActionDate DATE,
   ActionDescription VARCHAR(500)
);

