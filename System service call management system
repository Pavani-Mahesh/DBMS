--QUERIES:

--Queries using joins:

--1.Get the total number of service calls opened by each employee:
SELECT e.Name, COUNT(sc.ServiceCallID) AS TotalServiceCalls
FROM Employees e
LEFT JOIN ServiceCalls sc ON e.EmployeeID = sc.EmployeeID
GROUP BY e.Name;


SELECT t.Name, COUNT(sa.ServiceCallID) AS TotalAssignments
FROM Technicians t
INNER JOIN ServiceAssignments sa ON t.TechnicianID = sa.TechnicianID
GROUP BY t.Name
ORDER BY TotalAssignments DESC;

SELECT sc.ServiceCallID, c.Name AS CustomerName, t.Name AS TechnicianName
FROM ServiceCalls sc
INNER JOIN Customers c ON sc.CustomerID = c.CustomerID
INNER JOIN ServiceAssignments sa ON sc.ServiceCallID = sa.ServiceCallID
INNER JOIN Technicians t ON sa.TechnicianID = t.TechnicianID;

SELECT MONTH(DateOpened) AS Month, COUNT(ServiceCallID) AS TotalServiceCalls
FROM ServiceCalls
WHERE YEAR(DateOpened) = YEAR(GETDATE())
GROUP BY MONTH(DateOpened);

SELECT sc.ServiceCallID, c.Name AS CustomerName, sc.DateOpened, COUNT(sn.NoteID) AS TotalNotes
FROM ServiceCalls sc
INNER JOIN Customers c ON sc.CustomerID = c.CustomerID
LEFT JOIN ServiceCallNotes sn ON sc.ServiceCallID = sn.ServiceCallID
GROUP BY sc.ServiceCallID, c.Name, sc.DateOpened;

SELECT sc.ServiceCallID, AVG(sp.Price) AS AveragePartPrice
FROM ServiceCalls sc
INNER JOIN ServiceCallParts sparts ON sc.ServiceCallID = sparts.ServiceCallID
INNER JOIN SpareParts sp ON sparts.PartID = sp.PartID
GROUP BY sc.ServiceCallID;

SELECT sc.ServiceCallID, SUM(e.Amount) AS TotalExpenses
FROM ServiceCalls sc
LEFT JOIN Expenses e ON sc.ServiceCallID = e.ServiceCallID
GROUP BY sc.ServiceCallID;

SELECT sc.ServiceCallID, sc.Description, SUM(e.Amount) AS TotalExpenses
FROM ServiceCalls sc
INNER JOIN Expenses e ON sc.ServiceCallID = e.ServiceCallID
GROUP BY sc.ServiceCallID, sc.Description
HAVING SUM(e.Amount) > (SELECT AVG(Amount) FROM Expenses);


SELECT t.Name
FROM Technicians t
LEFT JOIN ServiceAssignments sa ON t.TechnicianID = sa.TechnicianID
WHERE sa.AssignmentID IS NULL;

SELECT sp.Name, sp.Description, sp.Price
FROM SpareParts sp
INNER JOIN ServiceCallParts scparts ON sp.PartID = scparts.PartID
ORDER BY sp.Price DESC;

SELECT e.Name
FROM Employees e
WHERE e.EmployeeID IN (
  SELECT sc.EmployeeID
  FROM ServiceCalls sc
  WHERE sc.EmployeeID = e.EmployeeID AND sc.DateClosed IS NOT NULL
) AND e.EmployeeID IN (
  SELECT sc.EmployeeID
  FROM ServiceCalls sc
  WHERE sc.EmployeeID = e.EmployeeID AND sc.DateClosed IS NULL
);

SELECT c.Name
FROM Customers c
WHERE c.CustomerID IN (
  SELECT co.CustomerID
  FROM Contracts co
  WHERE co.EndDate >= GETDATE()
);

SELECT t.Name
FROM Technicians t
WHERE t.TechnicianID IN (
  SELECT sa.TechnicianID
  FROM ServiceAssignments sa
  INNER JOIN ServiceCalls sc ON sa.ServiceCallID = sc.ServiceCallID
  INNER JOIN Contracts co ON sc.CustomerID = co.CustomerID
);

SELECT sc.ServiceCallID, c.Name AS CustomerName
FROM ServiceCalls sc
INNER JOIN Customers c ON sc.CustomerID = c.CustomerID
WHERE c.Email IN (
  SELECT e.Email
  FROM Employees e
);

SELECT COUNT(*) AS TotalCustomers
FROM Customers;

SELECT AVG(Price) AS AveragePrice
FROM Products;

SELECT MAX(Price) AS MaxPrice
FROM SpareParts;

SELECT MIN(StartDate) AS MinStartDate
FROM Contracts;

SELECT COUNT(*) AS TotalServiceCalls
FROM ServiceCalls;

SELECT SUM(Amount) AS TotalExpenses
FROM Expenses;

SELECT AVG(Quantity) AS AverageQuantity
FROM ServiceCallParts;

SELECT t.Name, COUNT(sa.AssignmentID) AS TotalAssignments
FROM Technicians t
INNER JOIN ServiceAssignments sa ON t.TechnicianID = sa.TechnicianID
GROUP BY t.Name
ORDER BY TotalAssignments DESC;

SELECT sc.ServiceCallID, c.Name AS CustomerName, sc.DateOpened
FROM ServiceCalls sc
INNER JOIN Customers c ON sc.CustomerID = c.CustomerID
WHERE sc.DateOpened = (SELECT MIN(DateOpened) FROM ServiceCalls);

SELECT t.Name AS TechnicianName, AVG(scp.Quantity) AS AverageQuantity
FROM Technicians t
INNER JOIN ServiceAssignments sa ON t.TechnicianID = sa.TechnicianID
INNER JOIN ServiceCalls sc ON sa.ServiceCallID = sc.ServiceCallID
INNER JOIN ServiceCallParts scp ON sc.ServiceCallID = scp.ServiceCallID
GROUP BY t.Name;

SELECT c.Name, SUM(e.Amount) AS TotalExpenses
FROM Customers c
INNER JOIN ServiceCalls sc ON c.CustomerID = sc.CustomerID
INNER JOIN Expenses e ON sc.ServiceCallID = e.ServiceCallID
GROUP BY c.Name
ORDER BY TotalExpenses DESC;

SELECT c.Name
FROM Customers c
WHERE EXISTS (
  SELECT 1
  FROM Contracts co
  WHERE co.CustomerID = c.CustomerID
    AND co.EndDate >= GETDATE()
);

SELECT sp.Name
FROM SpareParts sp
WHERE EXISTS (
  SELECT 1
  FROM ServiceCallParts sparts
  LEFT JOIN ContractServices cs ON sparts.PartID = cs.PartID
  WHERE sparts.PartID = sp.PartID
    AND cs.ContractID IS NULL
);

SELECT sc.ServiceCallID, c.Name AS CustomerName
FROM ServiceCalls sc
INNER JOIN Customers c ON sc.CustomerID = c.CustomerID
WHERE EXISTS (
  SELECT 1
  FROM Contracts co
  WHERE co.CustomerID = c.CustomerID
    AND co.EndDate >= GetDATE()
);

SELECT sc.ServiceCallID, sc.Description
FROM ServiceCalls sc
WHERE EXISTS (
  SELECT 1
  FROM ServiceCallNotes sn
  WHERE sn.ServiceCallID = sc.ServiceCallID
    AND sn.EmployeeID = sc.EmployeeID
);

SELECT sc.ServiceCallID, sc.Description
FROM ServiceCalls sc
WHERE (
  SELECT SUM(e.Amount)
  FROM Expenses e
  WHERE e.ServiceCallID = sc.ServiceCallID
) > (
  SELECT AVG(Amount)
  FROM Expenses
);

SELECT t.Name
FROM Technicians t
WHERE NOT EXISTS (
  SELECT 1
  FROM ServiceAssignments sa
  WHERE sa.TechnicianID = t.TechnicianID
);


SELECT Name
FROM Customers
WHERE Name LIKE 'A%';

SELECT Name
FROM Employees
WHERE Name LIKE '%son';

SELECT Name
FROM Technicians
WHERE Name LIKE '%e%';

SELECT Name
FROM Products
WHERE Name LIKE 'S%e';

SELECT Name
FROM Customers
WHERE Name LIKE '_o%o%';


SELECT c.Name, SUM(e.Amount) AS TotalExpenses
FROM Customers c
JOIN ServiceCalls sc ON c.CustomerID = sc.CustomerID
JOIN Expenses e ON sc.ServiceCallID = e.ServiceCallID
GROUP BY c.CustomerID, c.Name
ORDER BY TotalExpenses DESC;

SELECT LEFT(Name, 1) AS FirstLetter, AVG(Price) AS AveragePrice
FROM Products
GROUP BY Name;

SELECT sc.ServiceCallID, c.Name AS CustomerName, e.Name AS EmployeeName, SUM(exp.Amount) AS TotalExpenses
FROM ServiceCalls sc
JOIN Customers c ON sc.CustomerID = c.CustomerID
JOIN Employees e ON sc.EmployeeID = e.EmployeeID
JOIN Expenses exp ON sc.ServiceCallID = exp.ServiceCallID
GROUP BY sc.ServiceCallID, c.Name, e.Name
ORDER BY TotalExpenses DESC;

SELECT t.Name
FROM Technicians t
JOIN ServiceAssignments sa ON t.TechnicianID = sa.TechnicianID
GROUP BY t.Name
HAVING COUNT(sa.ServiceCallID) >= 2;

SELECT sc.ServiceCallID, COUNT(sn.NoteID) AS NoteCount
FROM ServiceCalls sc
JOIN ServiceCallNotes sn ON sc.ServiceCallID = sn.ServiceCallID
GROUP BY sc.ServiceCallID
ORDER BY NoteCount DESC;

SELECT ProductID, Name, Price
FROM Products
WHERE Price > (
  SELECT AVG(Price)
  FROM Products
);

SELECT ServiceCallID, AVG(Quantity) AS AverageQuantity
FROM ServiceCallParts
GROUP BY ServiceCallID;

SELECT ServiceCallID, DateOpened
FROM ServiceCalls
ORDER BY DateOpened ASC;

SELECT ServiceCallID, AVG(sp.Price) AS AveragePrice
FROM ServiceCallParts scp
JOIN SpareParts sp ON scp.PartID = sp.PartID
GROUP BY ServiceCallID;

SELECT e.Name
FROM Employees e
LEFT JOIN ServiceCalls sc ON e.EmployeeID = sc.EmployeeID
WHERE sc.ServiceCallID IS NULL;

SELECT ServiceCallID, SUM(Quantity) AS TotalPartsUsed
FROM ServiceCallParts
GROUP BY ServiceCallID
ORDER BY ServiceCallID;

SELECT e1.Name, e2.Name
FROM Employees e1
JOIN Employees e2 ON e1.Position = e2.Position AND e1.Department = e2.Department
WHERE e1.EmployeeID <> e2.EmployeeID;

SELECT e.Name, sc.Description
FROM Employees e
JOIN ServiceCalls sc ON e.EmployeeID = sc.EmployeeID
GROUP BY e.Name, sc.Description
HAVING COUNT(DISTINCT sc.ServiceCallID) > 1;

SELECT sp.PartID, sp.Name, COUNT(scp.PartID) AS UsageCount
FROM SpareParts sp
JOIN ServiceCallParts scp ON sp.PartID = scp.PartID
GROUP BY sp.PartID, sp.Name
ORDER BY UsageCount DESC;


SELECT e.Name
FROM Employees e
JOIN ServiceCalls sc ON e.EmployeeID = sc.EmployeeID
GROUP BY e.Name
HAVING COUNT(DISTINCT sc.CustomerID) <2;
