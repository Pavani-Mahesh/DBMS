CREATE TABLE Contracts (
   ContractID INT PRIMARY KEY,
   CustomerID INT FOREIGN KEY REFERENCES Customers(CustomerID),
   StartDate DATE,
   EndDate DATE,
   Description VARCHAR(500)
);

